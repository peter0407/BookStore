            <BorderPane prefHeight="200.0" prefWidth="200.0">
               <left>
                  <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER" />
               </left>
               <top>
                  <Label text="Home" textFill="#4bc3eb" BorderPane.alignment="CENTER_LEFT">
                     <font>
                        <Font size="36.0" />
                     </font>
                     <BorderPane.margin>
                        <Insets left="100.0" />
                     </BorderPane.margin>
                  </Label>
               </top>
               <center>
